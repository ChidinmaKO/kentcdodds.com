{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "$components/*": ["src/components/*"]
    }
  },
  "include": ["./src/**/*", "./posts/**/*", "./test/**/*", "./typings/**/*"]
}
