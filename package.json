  "author": "Kent C. Dodds <kent@doddsfamily.us>",
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@weknow/gatsby-remark-twitter": "^0.1.2",
    "emotion": "^10.0.5",
    "emotion-server": "^10.0.5",
    "gatsby": "^2.0.53",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "^0.3.1-ci.227",
    "gatsby-plugin-emotion": "^3.0.1",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-helmet": "^5.2.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "serve --no-clipboard --listen 8899 public",
    "lint": "eslint .",
    "tweet-to-html": "pbpaste | node ./other/tweet-to-html.js | pbcopy",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --build ./tsconfig.json cypress/tsconfig.json",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:ci": "echo this does not work yet...",
    "test:e2e:ci:future": "cross-env CYPRESS_BASE_URL=http://localhost:8899 start-server-and-test start http://localhost:8899 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8000 cy:dev",
    "cy:run": "cypress run",
    "cy:dev": "cypress open",
    "setup": "yarn && yarn validate",
    "validate": "concurrently --kill-others-on-fail --prefix [{name}] --names test:e2e,lint,test,typecheck --prefix-colors bgBlue.bold.reset,bgGreen.bold.reset,bgMagenta.bold.reset,bgCyan.bold.reset \"npm run test:e2e:ci --silent\" \"npm run lint --silent\" \"npm run test:coverage --silent\" \"npm run typecheck --silent\"",
    "format": "prettier --write \"src/**/*.+(js|css|ts|tsx|html)\""
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@types/jest": "^23.3.10",
    "@types/jest-in-case": "^1.0.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.3",
    "cypress-testing-library": "^2.3.4",
    "eslint": "^5.10.0",
    "eslint-config-kentcdodds": "^14.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "is-ci-cli": "^1.1.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-emotion": "^10.0.5",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.7.1",
    "jest-runner-tsc": "^1.3.2",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.2.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.2",
    "react-testing-library": "^5.4.0",
    "serve": "^10.1.1",
    "start-server-and-test": "^1.7.11",
    "typescript": "^3.2.2"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/react",
      "kentcdodds/jest"
    ],
    "rules": {
      "no-use-before-define": "off",
      "react/jsx-pascal-case": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
